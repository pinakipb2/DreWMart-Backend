{
  "name": "drewmart_backend",
  "version": "1.0.0",
  "description": "DrewMart Backend",
  "main": "app.js",
  "scripts": {
    "start": "node build/app.js",
    "dev": "nodemon --exec babel-node src/app.js",
    "build": "babel src -d build",
    "addProd": "babel-node src/manageProduct/addProducts.js",
    "addCouponItems": "babel-node src/manageProduct/addCouponItems.js",
    "dropProd": "babel-node src/manageProduct/dropProducts.js",
    "dropAll": "babel-node src/manageProduct/dropAll.js",
    "lint": "eslint src/**/*.js",
    "lint-fix": "exlint src/**/*.js --fix",
    "reset": "npm run dropAll && npm run addProd"
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^4.5.0",
    "axios": "^1.1.3",
    "cors": "^2.8.5",
    "cuid": "^2.1.8",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.6.0",
    "helmet": "^6.0.0",
    "http-errors": "^2.0.0",
    "joi": "^17.7.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "uuid": "^9.0.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/node": "^7.19.1",
    "@babel/preset-env": "^7.19.4",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "nodemon": "^2.0.20",
    "prisma": "^4.5.0"
  }
}
